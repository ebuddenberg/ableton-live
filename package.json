{
  "name": "ableton-live-downloader",
  "version": "1.0.0",
  "description": "Downloads the desired 64-bit installer from Ableton Live servers",
  "author": "Pieter Montoulieu",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "build-deploy": "nuxt generate; push-dir --dir=dist --branch=gh-pages --cleanup",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-rc.11",
    "cross-env": "^5.2.0",
    "nuxt": "^2.3.4",
    "push-dir": "^0.4.1"
  },
  "devDependencies": {
    "nodemon": "^1.18.9"
  }
}
